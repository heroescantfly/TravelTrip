@using TravelTripProject.Models.Sınıflar
@model List<Blog>

<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    <ul class="list list-pad">
        @foreach (var x in Model)
        {
            <li class="list-bg"><span>@x.ID.</span><a href="/Blog/BlogDetay/@x.ID">@x.Baslik</a></li>

        }
    </ul>
    <a href="/Blog/" class="link1">Bütün Blog'ları Gör</a>
</div>
